{
  "name": "webeenthere",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && node server.js",
    "client": "cd client && set NODE_OPTIONS=--max-old-space-size=4096 && npm run dev",
    "start": "npm run dev",
    "build": "npm run install-all && npm run build-client",
    "build-client": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "init-db": "cd server && npm run init-db",
    "db-status": "cd server && npm run db-status",
    "backup": "cd server && node -e \"require('./backupService').createBackup().then(r => console.log(r))\"",
    "list-backups": "cd server && node -e \"console.log(require('./backupService').listBackups())\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kurisu21/webeenthere.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kurisu21/webeenthere/issues"
  },
  "homepage": "https://github.com/Kurisu21/webeenthere#readme",
  "description": "",
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
