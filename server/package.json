    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.6.0",
    "next-auth": "^4.24.11",
    "nodemailer": "^7.0.6",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
