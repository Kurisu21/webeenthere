{
  "name": "webeenthere-server",
  "version": "1.0.0",
  "description": "Backend for Webeenthere (Express, MySQL, OOP)",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "echo 'No build step required for server'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "init-db": "node database/initDatabase.js",
